{
  "manifest_version": 3,
  "name": "Strophe",
  "version": "v0.1.3",
  "description": "Strophe - make newtab a place for your thoughts and notes",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "author": {
    "email": "renardeinside@gmail.com"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "favicon-16x16.png",
    "32": "favicon-32x32.png",
    "57": "favicon-57x57.png",
    "72": "favicon-72x72.png",
    "96": "favicon-96x96.png",
    "114": "favicon-114x114.png"
  }
}